function globalsConfig(viewsUrl,mainjs,initData){
    window.globalData = initData;
    define("views/global/common/Model", {globalData:initData});
    //如果不是工程商，则跳转到用户手机版首页；
    if(globalData.user.user_type != 100 && globalData.user.user_type != 200){
        window.location.href = "`{MSITE}`";
        return;
    }

    var nodeModules = viewsUrl+"/node_modules"
    require.config({
        paths: @@include('./node_modules.json')
    });
    require.config({
        baseUrl : viewsUrl,
        paths:{
            "views/global" : "global",
            "views/www" : "www",
            "views/m" : "m"
        }
    });

    var __requireJS = function(path,basepath){
        basepath = basepath || nodeModules;
        document.write('<script src="'+basepath+path+'"><\/script>');
    }
    //__requireJS('/bootstrap.js');
    //document.write('<script>require(["'+mainjs+'"]);<\/script>');

    require([mainjs]);

    webimConfigs.login();
}
